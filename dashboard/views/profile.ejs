    color: #3a6cf4;
    font-size: 1.2em;
    font-weight: 700;
    margin: 10px;
    color: white;
}
.projects {
    background-color: #000016;
}
.projects .content{
    margin-top: 30px;
}
.project-card {
    background-color: #fff;
    border: 1px solid #fff;
    min-height: 14em;
    width: 23em;
    overflow: hidden;
    border-radius: 10px;
    margin: 20px;
    transition: 0.7s ease;
}
.project-card:hover {
   transform: scale(1.1);
}
.project-card:hover .project-image {
    opacity: 0.9;
 }
.project-image img{
    width: 100%
}
